var ViDApp = angular.module('ViDApp');
ViDApp.controller('NoteModalInstanceCtrl', function ($uibModalInstance, noteService, dataService) {
  var $ctrl = this;
  $ctrl.title = 'Notes for QA';
  $ctrl.noteService = noteService;
  $ctrl.dataService = dataService;

  $ctrl.cancel = function() {
    $uibModalInstance.dismiss('cancel');
  };

  $ctrl.ok = function() {
    $uibModalInstance.close('NoteSaved');
  }
});
