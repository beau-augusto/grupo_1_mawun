var app = angular.module('ViDApp');
app.service('noteService', ['$uibModal',
                             'dataService',
  function(
    $uibModal,
    dataService) {

  this.dataService = dataService

  this.openNoteModal = function() {
    $uibModal.open({
      ariaLabelledBy: 'modal-title-bottom',
      ariaDescribedBy: 'modal-body-bottom',
      templateUrl: 'note-modal.html',
      size: 'med',
      controller: 'NoteModalInstanceCtrl',
      controllerAs: '$ctrl'
    });
  }
}]);
