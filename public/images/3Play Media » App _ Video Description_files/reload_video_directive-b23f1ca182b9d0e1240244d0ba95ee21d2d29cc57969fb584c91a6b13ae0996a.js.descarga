var app = angular.module('ViDApp');
app.directive('reloadVideo', ['$timeout', 'videoService', function ($timeout, videoService) {
  return {
    link: function ($scope, element, attrs) {
      $scope.$on('page-data-loaded', function () {
        $timeout(function () {
          videoService.resetP3()
        }, 0, false);
      })
    }
  };
}]);
