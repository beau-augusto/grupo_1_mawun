var ViDApp = angular.module('ViDApp');
ViDApp.controller('ResynthesizeModalInstanceCtrl', function ($uibModalInstance, title, body, snippetService) {
  var $ctrl = this;
  $ctrl.title = title;
  $ctrl.body = body;
  $ctrl.snippetService = snippetService;

  $ctrl.resynthesize = function () {
    $uibModalInstance.close();
  	$ctrl.snippetService.resynthesizeAll();
  };

  $ctrl.cancel = function () {
    $uibModalInstance.close(false);
  };
});
